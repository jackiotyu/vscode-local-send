{
  "name": "vscode-local-send",
  "publisher": "jackiotyu",
  "displayName": "LocalSend (unofficial)",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jackiotyu/vscode-local-send.git"
  },
  "bugs": {
    "url": "https://github.com/jackiotyu/vscode-local-send/issues"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-local-send.startServer",
        "title": "Start LocalSend Server"
      },
      {
        "command": "vscode-local-send.stopServer",
        "title": "Stop LocalSend Server"
      }
    ],
    "configuration": {
      "title": "LocalSend",
      "properties": {
        "localSend.port": {
          "type": "number",
          "default": 3399,
          "description": "LocalSend server port"
        },
        "localSend.deviceName": {
          "type": "string",
          "default": "VSCode",
          "description": "Device name shown in LocalSend network"
        }
      }
    }
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vsce:pack": "vsce package --no-dependencies",
    "vscode:prepublish": "pnpm run package",
    "compile": "rspack",
    "watch": "rspack --watch",
    "package": "rspack --mode production --devtool true",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@rspack/cli": "^1.2.8",
    "@rspack/core": "^1.2.8",
    "@swc/helpers": "^0.5.15",
    "@types/mocha": "^10.0.10",
    "@types/node": "18.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "pino-pretty": "^13.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "fastify": "^5.2.2",
    "fastify-raw-body": "^5.0.0",
    "form-data": "^4.0.2"
  }
}
